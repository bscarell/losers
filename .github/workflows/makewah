name: C/C++ CI

on:
  push:
    branches: [ "master" ]
  pull_request:
    branches: [ "master" ]

jobs:
  build:

    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v3
    - name: make
      run: make
    - name: make wah
      run: make wah
    - name: make dist
      run: make dist
    - name: OUTPUT
      run: echo "START"; echo "MAKE WAH" && make wah && echo "SUCCESS!"; echo "done... see if success is echo'd above"
